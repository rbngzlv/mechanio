.container
  .row
    / Left Navigation Part
    .col-md-3.nav-collapse
      %ul.nav.nav-pills.nav-stacked
        %li.active
          = link_to 'mechanic-dashboard.html' do
            %span.img-circle  
              %i.glyphicon.glyphicon-dashboard
            Dashboard
        %li
          = link_to 'mechanic-profile.html' do
            %span.img-circle  
              %i.glyphicon.glyphicon-user
            My Profile
        %li
          = link_to 'mechanic-jobs.html' do
            %span.img-circle  
              %i.glyphicon.glyphicon-thumbs-up
            My Jobs
        %li
          = link_to 'mechanic-payout.html' do
            %span.img-circle  
              %i.glyphicon.glyphicon-credit-card
            Payout Information
        %li
          = link_to 'mechanic-settings.html' do
            %span.img-circle  
              %i.glyphicon.glyphicon-calendar
            Settings

    / Right Content Part
    %section.col-md-9#main-content
      / Profile Part
      .row
        .col-md-12
          .panel.panel-default
            .panel-heading.clearfix
              %h4.panel-title
                %i.glyphicon.glyphicon-user
                Bob Mechanic
              %span.tools.pull-right
                %a.glyphicon.glyphicon-chevron-down{href: "javascript:;"}
            .panel-body.mechanic-panel-body
              .col-md-4
                .profile-border
                  %img(src="../../assets/photo.jpg")
                  .ReviewsRow
                    .stars
                      %span.Color &nbsp;
                      %span.Color &nbsp;
                      %span.Blank &nbsp;
                      %span.Blank &nbsp;
                      %span.Blank &nbsp;                      
                    %span.revieText 12 Reviews
              .col-md-8.panel.nested-panel
                %h5 Hi, I'm your Mechanic Bob Mechanic
                %p Punched Russell Crowe in the face onece. Felt Nice.

      / Upcoming Jobs Part
      .row
        .col-md-12
          .panel.panel-default
            .panel-heading.clearfix
              %h4.panel-title
                %i.glyphicon.glyphicon-user
                %a(href="mechanic-jobs.html#scheduled-jobs")
                  Upcoming Jobs
              %span.tools.pull-right
                %a.glyphicon.glyphicon-chevron-down(href="javascript:;")
            .panel-body
              .panel.nested-panel
                .col-md-7.panel-right-border
                  %p
                    %b.text-info
                      Andrew Nandjroo
                  %p
                    %i.glyphicon.glyphicon-play
                    2000 Ford Mustang V6-3 OL
                  %p
                    %b Services:
                    Car is making a noise (inspection)
