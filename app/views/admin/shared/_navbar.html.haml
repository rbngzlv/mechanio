.navbar.navbar-default.navbar-fixed-top{ role: 'navigation' }
  .container
    = link_to 'Mechanio', root_path, class: 'navbar-brand'
    .navbar-right
      %ul.nav.navbar-nav
      - if admin_signed_in?
        = link_to 'Logout', destroy_admin_session_path, method: :delete, class: 'btn btn-default navbar-btn'
      - else
        = link_to 'Login', new_admin_session_path, class: 'btn btn-default navbar-btn'
