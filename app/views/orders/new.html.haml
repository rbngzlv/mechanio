.container
  .col-md-9.cornsilk
    .panel.panel-default.panel-wizard.panel-rounded
      .panel-body
        %ul.nav.nav-pills.nav-justified.nav-pills-wizard
          %li.done.disabled
            = link_to '#' do
              %span 1
              CarDetails
          %li.active.disabled
            = link_to '#' do
              %span 2
              Diagnose
          %li.disabled
            = link_to '#' do
              %span 3
              Contact
          %li.disabled
            = link_to '#' do
              %span 4
              Quote
        %h2.hx-default ADD YOUR VEHICLE
        %hr.hr-dark-gray
        %form{role: "form"}
          .row
            .form-group.col-md-3
              %select.form-control{"data-placeholder" => "Year", tabindex: "1"}
                %option{value: "Category 1"} Year
                %option{value: "Category 2"} 2013
                %option{value: "Category 3"} 2012
                %option{value: "Category 4"} 2011
          .row
            .form-group.col-md-3
              %select.form-control{"data-placeholder" => "Ford", tabindex: "1"}
                %option{value: "Category 1"} Ford
                %option{value: "Category 2"} Category 2
                %option{value: "Category 3"} Category 5
                %option{value: "Category 4"} Category 4
          .row
            .form-group.col-md-3
              %select.form-control{"data-placeholder" => "ABC", tabindex: "1"}
                %option{value: "Category 1"} ABC
                %option{value: "Category 2"} Category 2
                %option{value: "Category 3"} Category 5
                %option{value: "Category 4"} Category 4
          .row
            .form-group.col-md-3
              %select.form-control{"data-placeholder" => "Sub Model", tabindex: "1"}
                %option{value: "Category 1"} Sub Model
                %option{value: "Category 2"} Category 2
                %option{value: "Category 3"} Category 5
                %option{value: "Category 4"} Category 4
          .row
            .form-group.col-md-3
              %select.form-control{"data-placeholder" => "Engine", tabindex: "1"}
                %option{value: "Category 1"} Engine
                %option{value: "Category 2"} Category 2
                %option{value: "Category 3"} Category 5
                %option{value: "Category 4"} Category 4
          %hr
          %button.btn.btn-default.btn-back
            %small &#x276E
            Back
          %button.btn.btn-primary.pull-right
            Continue
            %small &#x276F
  .col-md-3.lightgreen
    .panel.panel-default.panel-rounded
      .panel-body
        %h3.hx-default.panel-title YOU'VE ANSWERED
        %hr.hr-dark-gray
        .progress.progress-rounded
          .progress-bar.progress-bar-gradinet(role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 60%;")
            %span.sr-only 60% Complete
    .panel.panel-default.panel-rounded
      .panel-body
        %h3.hx-default.panel-title VEHICLE
        %hr.hr-dark-gray
        2010 Cadillac DTS L8 Cyl 4.6L
    .panel.panel-default.panel-rounded
      .panel-body
        %h3.hx-default.panel-title CAR SERVICING
        %hr.hr-dark-gray
        30,000km servicing
