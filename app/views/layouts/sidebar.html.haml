!!!
%html
  = render 'layouts/head'
  %body
    .wrap
      = render 'shared/header'
      .container
        - if user_signed_in?
          = render 'shared/client_nav'
        - elsif mechanic_signed_in?
          = render 'shared/mechanics_nav'
        .col-md-9
          = render 'shared/flash_messages'
          = yield
    = render 'shared/footer'
